
// Objectives
// 1. Exploring Method Overriding

void main() {
  
  var dog = Dog();
  dog.eat();        //method overiding
  print(dog.color);  // property overiding
  

}

class Animal
{
  String color = "brown";
  void eat()
  {
    print("Animal is eating");
  }
}

class Dog extends Animal
{
  String color = "Black";
  void eat()
  {
    super.eat();
    print("Dog is eating");
    
  }
}
